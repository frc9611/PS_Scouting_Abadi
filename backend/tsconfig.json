{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "resolveJsonModule": true // Deixei em true para "autorizar" a importação de Json, em false pode gerar o erro com a seguinte menção "--resolveJsonModule"
  }
}
